{
    "directories": [
        "~/.vim.d"
    ],

    "link": {
        "files/vimrc": "~/.vimrc",
	"files/iterm2_profiles.json": "~/Library/Application Support/iTerm2/DynamicProfiles/iterm2_profiles.json"
    },

    "commands": [
        "./scripts/git-first-clone.sh https://github.com/VundleVim/Vundle.vim.git ~/.vim.d/bundle/Vundle.vim",
	"vim +PluginInstall +qall"
    ]
}
